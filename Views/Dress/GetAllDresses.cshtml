@model IEnumerable<BoutiqueShop.Models.Dress>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DressName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DressPrice)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ShopId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DressName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DressPrice)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ShopId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.DressID }) |
            @Html.ActionLink("Details", "Details", new { id=item.DressID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.DressID })
        </td>
    </tr>
}

</table>
